{
  "name": "@krista/webpack-utils",
  "version": "1.0.0",
  "description": "@krista/webpack workflow",
  "main": "lib/index.js",
  "scripts": {
    "preversion": "npm run lib",
    "version": "git add ./package.json && git add ./npm-shrinkwrap.json && git commit && git flow release start",
    "postversion": "git flow release publish",
    "prepublish": "npm run lib",
    "postpublish": "git flow release finish && git push --tags",
    "lib": "./node_modules/.bin/tsc -p ./src/main/ts",
    "tsd": "./node_modules/.bin/tsd install --config ./.config/tsd.json"
  },
  "engines": {
    "node": ">=v0.12.5"
  },
  "author": "agaricus <krutin@krista.ru>",
  "license": "ISC",
  "devDependencies": {
    "tsd": "^0.6.4",
    "typescript": "^1.7.0-dev.20150919"
  },
  "dependencies": {
    "webpack": "^1.12.2"
  }
}
